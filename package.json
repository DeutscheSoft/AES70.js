{
  "name": "aes70",
  "version": "1.3.8",
  "description": "A controller library for the AES70 protocol.",
  "type": "module",
  "main": "src/index.js",
  "module": "src/index.js",
  "browser": "dist/AES70.es5.js",
  "browser:module": "src/index.browser.js",
  "typings": "src/index.d.ts",
  "exports": {
    ".": {
      "node": "./src/index.js",
      "default": "./src/index.browser.js"
    },
    "./RemoteControlClasses": "./src/controller/ControlClasses.js",
    "./OCP1": "./src/OCP1.js",
    "./AES70.es5.js": "./dist/AES70.es5.js",
    "./*": "./src/*"
  },
  "bin": {
    "printDevice": "bin/printDevice.js"
  },
  "directories": {
    "src": "src"
  },
  "scripts": {
    "test": "node tests/signatures.js",
    "prepublishOnly": "make",
    "docs": "cd esdoc && npm run build",
    "prettier": "cd prettier && npm run prettier",
    "eslint": "./eslint/node_modules/.bin/eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DeutscheSoft/AES70.js.git"
  },
  "keywords": [
    "Audio",
    "Control",
    "Protocol",
    "RPC"
  ],
  "author": {
    "name": "Arne Goedeke",
    "email": "arne@deuso.de"
  },
  "license": "GPL-2.0",
  "bugs": {
    "url": "https://github.com/DeutscheSoft/AES70.js/issues"
  },
  "homepage": "https://github.com/DeutscheSoft/AES70.js#readme",
  "dependencies": {},
  "optionalDependencies": {
    "ws": "^5.2.2",
    "dnssd": "^0.3.3"
  },
  "sideEffects": [
    "src/bundle.browser.js",
    "dist/AES70.es5.js"
  ],
  "devDependencies": {}
}
